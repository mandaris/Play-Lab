{
  "params": {
      "themeVersion": "0.0.1"
  },
  "markup": {
    "highlight": {
      "noClasses": false
    },
    "goldmark": {
      "parser": {
        "wrapStandAloneImageWithinParagraph": false,
        "attribute": {
          "block": true
        }
      },
      "extensions": {
        "passthrough": {
          "delimiters": {
            "block": [
              [
                "\\[",
                "\\]"
              ],
              [
                "$$",
                "$$"
              ]
            ],
            "inline": [
              [
                "\\(",
                "\\)"
              ]
            ]
          },
          "enable": true
        }
      }
    }
  }
}